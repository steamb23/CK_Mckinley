<Window x:Class="CK_Mckinley.SelectDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CK_Mckinley"
        mc:Ignorable="d"
        Title="검색 결과" Height="400" Width="533" WindowStartupLocation="CenterScreen" MinWidth="533" MinHeight="400">
    <Grid MouseLeftButtonDown="Grid_MouseLeftButtonDown">
        <DataGrid x:Name="dataGrid" Margin="10,10,10,55" Loaded="dataGrid_Loaded" IsReadOnly="True" ColumnWidth="Auto" />
        <Button x:Name="button_ok" Content="확인" HorizontalAlignment="Right" Margin="10" VerticalAlignment="Bottom" Width="80" Height="40" Click="button_ok_Click"/>
    </Grid>
</Window>
